@model AgentPortal.ViewModels.Agent.AgentListViewModel


<table class="table table-bordered table-responsive table-hover">
    <tr>
        <th>Agent Code </th>
        <th>Agent Name </th>
        <th>Commission</th>
        <th>Phone Number</th>
        <th>Working Area</th>
    </tr>
    @foreach (var agent in Model.agents)
    {
        <tr>
            <td><a href="/Home/Agent/@agent.AgentCode">@agent.AgentCode</a></td>
            <td>@agent.AgentName</td>
            <td>@agent.Commission</td>
            <td>@agent.PhoneNumber</td>
            <td>@agent.WorkingArea</td>
        </tr>
    }
</table>  